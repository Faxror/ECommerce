@model List<ResultCategoryDto>
@{
    Layout = null;
}

<div class="container">
    <table class="table table-bordered">
        <tr>
            <th>#</th>
            <th>Kategory Adu</th>
        </tr>

        @foreach(var item in Model) {
            <tr>
                <td>@item.CategoryID</td>
                <td>@item.CategoryName</td>
            </tr>
        }
    </table>
</div>